{
    "_version": "1.58.0",
    "sap.app": {
        "id": "ui5.walkthrough",
        "type": "application",
        "title": "Eine App",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "dataSources": {
            "oDataService": {
                "uri": "/sap/opu/odata/sap/ZOSO_PURCHASEORDER/",
                "type": "OData"
            }
        }
    },
    "sap.ui5": {
        "models": {
            "": {
                "dataSource": "oDataService"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/styles.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "ui5.walkthrough.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteHomeView",
                    "pattern": "",
                    "target": [
                        "TargetHomeView"
                    ]
                },
                
                {
                    "name": "Detail",
                    "pattern": "Detail/{orderId}",
                    "target": [
                        "TargetDetailView"
                    ]
                }
            ],
            "targets": {
                "TargetHomeView": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Home",
                    "viewName": "Home"
                },
            
                "TargetDetailView": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Detail",
                    "viewName": "Detail"
                }
            }
        },
        "rootView": {
            "viewName": "ui5.walkthrough.view.App",
            "type": "XML"
        }
    }
}