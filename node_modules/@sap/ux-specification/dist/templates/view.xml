<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	xmlns:html="http://www.w3.org/1999/xhtml"<%- controllerName ? ' controllerName="' + controllerName + '"' : '' %>>
	<%if (page) { %>
	<App>
		<pages>
			<Page title="<%= name %>">
				<content></content>
			</Page>
		</pages>
	</App>
	<% } else { %>
	<Label text="<%= name %>" />
	<% } %>
</mvc:View>